//
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        // Crear un objeto Scanner para obtener entrada desde la línea de comandos
        Scanner input = new Scanner(System.in);

        // Solicitar al usuario ingresar los números
        System.out.print("Ingrese el primer número entero: ");
        int number1 = input.nextInt(); // Leer el primer número

        System.out.print("Ingrese el segundo número entero: ");
        int number2 = input.nextInt(); // Leer el segundo número

        // Comparaciones entre los números ingresados
        if (number1 == number2) {
            System.out.printf("%d == %d%n", number1, number2); // Igualdad
        }
        if (number1 != number2) {
            System.out.printf("%d != %d%n", number1, number2); // Desigualdad
        }
        if (number1 < number2) {
            System.out.printf("%d < %d%n", number1, number2); // Menor que
        }
        if (number1 > number2) {
            System.out.printf("%d > %d%n", number1, number2); // Mayor que
        }
        if (number1 <= number2) {
            System.out.printf("%d <= %d%n", number1, number2); // Menor o igual que
        }
        if (number1 >= number2) {
            System.out.printf("%d >= %d%n", number1, number2); // Mayor o igual que
        }

        // Cerrar el objeto Scanner
        input.close();
    }
}
